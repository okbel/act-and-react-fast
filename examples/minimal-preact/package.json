{
  "name": "minimal-peact",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node server",
    "dev": "node webpack-dev-server",
    "build": "rm -rf build/* | NODE_ENV=production webpack -p --progress --env.prod",
    "buildH": "rm -rf build/* | NODE_ENV=production webpack --progress --env.prod"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "babel-plugin-transform-async-to-generator",
      "babel-plugin-transform-runtime",
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "dependencies": {
    "express": "^4.14.0",
    "preact": "^6.4.0",
    "preact-router": "^2.3.2",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-generator": "6.19.0",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-runtime": "6.18.0",
    "html-webpack-plugin": "^2.24.1",
    "webpack": "v2.1.0-beta.25",
    "webpack-dev-server": "v2.1.0-beta.10"
  }
}
